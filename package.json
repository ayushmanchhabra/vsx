{
  "name": "vsx",
  "version": "0.8.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayushman Chhabra",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest run --coverage.enabled true"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^24.9.2",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.6",
    "esbuild": "~0.25.12",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "volta": {
    "node": "24.11.0",
    "npm": "11.6.2"
  },
  "workspaces": [
    "templates/*"
  ]
}
