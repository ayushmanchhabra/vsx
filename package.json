{
  "name": "vsx",
  "version": "0.8.0",
  "description": "Write vanilla JavaScript in JSX.",
  "author": {
    "name": "Ayushman Chhabra",
    "url": "https://ayushmanchhabra.com"
  },
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "src"
  ],
  "homepage": "https://github.com/ayushmanchhabra/vsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayushmanchhabra/vsx.git"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.ts",
    "build": "tsup src/index.ts --minify --format cjs,esm --dts --clean",
    "test": "vitest run --coverage.enabled true"
  },
  "keywords": [
    "JSX",
    "JavaScript"
  ],
  "dependencies": {
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.16.5",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "~0.25.8",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.17.1",
    "npm": "11.4.2"
  },
  "workspaces": [
    "templates/*"
  ]
}
